    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/stylis": "^4.0.2",
    "esbuild": "^0.15.7",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
