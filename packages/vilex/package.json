{
  "name": "vilex",
  "version": "0.3.1",
  "description": "The JavaScript framework for building modern web UI.",
  "main": "./dist/vii.js",
  "module": "./dist/vii.js",
  "types": "./dist/vii.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
